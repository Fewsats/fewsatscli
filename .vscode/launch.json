{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Upload",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/cli/main.go",
            "args": [
                "--verbose",
                "storage", "upload",
                "--name", "Steps Towards Artificial Intelligence",
                "--price", "0.05",
                "--file-path", "${workspaceFolder}/minsky.pdf",
                "--description", "Steps Towards Artificial Intelligence paper by Marvin Minsky 1961",
                "--cover-image", "${workspaceFolder}/Zhuangzi.png"
            ]
        },
        {
            "name": "Download",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/cli/main.go",
            "args": [
                "--verbose",
                "--profile", "staging",
                "storage", "download",
                "7d8400e5-d951-4aaf-b365-2840676f70b4"

            ],
            "console": "externalTerminal"
        },
        {
            "name": "Create account",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/cli/main.go",
            "args": [
                "account", "signup",
            ],
            "console": "externalTerminal"
        },
        {
            "name": "Login",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/cli/main.go",
            "args": [
                "--profile", "staging",
                "account", "login",
            ],
            "console": "externalTerminal"
        },
        {
            "name": "List API keys",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/cli/main.go",
            "args": [
                "--profile", "production",
                "apikeys", "list",
            ],
        },
        {
            "name": "Update Gateway",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/cli/main.go",
            "args": [
                "gateway", "update",
                "--id", "4e1a4fb4-111d-442c-acba-a0152147e4ec",
                "--name", "test",
                "--price", "69",
            ],
        },
        {
            "name": "Create API key",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/cli/main.go",
            "args": [
                "--verbose",
                "apikeys", "new",
            ],
            "console": "externalTerminal"
        },
        {
            "name": "Connect Wallet",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/cli/main.go",
            "args": [
                "wallet", "connect",
                "--type", "alby",
                "--token", "this"
            ],
        }
    ]
}
